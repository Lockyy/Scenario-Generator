<!-- Project Wonderful Ad Box Loader -->
<script type="text/javascript">
  var ready;
  ready = function() {

    if(arguments.callee.z)return;else arguments.callee.z=true;
      var d=document;var s=d.createElement('script');
      var x=d.getElementsByTagName('script')[0];
      s.type='text/javascript';s.async=true;
      s.src='//www.projectwonderful.com/pwa.js';
      x.parentNode.insertBefore(s,x);

  };

  $(document).ready(ready);
  $(document).on('page:load', ready);
  
  (function(){function pw_load(){
    if(arguments.callee.z)return;else arguments.callee.z=true;
    var d=document;var s=d.createElement('script');
    var x=d.getElementsByTagName('script')[0];
    s.type='text/javascript';s.async=true;
    s.src='//www.projectwonderful.com/pwa.js';
    x.parentNode.insertBefore(s,x);}
  if (window.attachEvent){
    window.attachEvent('DOMContentLoaded',pw_load);
    window.attachEvent('onload',pw_load);}
  else{
    window.addEventListener('DOMContentLoaded',pw_load,false);
    window.addEventListener('load',pw_load,false);}})();

  <!-- http://www.labnol.org/internet/alternate-content-for-adblock/28602/ -->
  adblock = function(){ 
    setTimeout(function() { 
      var google_ad = document.querySelector("ins.adsbygoogle");
      var wonderful_ad = $('#pw_adbox_75597_1_0');

      if (google_ad && google_ad.innerHTML.replace(/\s/g, "").length == 0 && $(wonderful_ad).children().length == 0) {
        console.log('hiding');
        $('.request').show();
        $('.hide-on-blocking').hide();
      }
    }, 800);
  };

  $(document).ready(adblock);
  $(document).on('page:load', adblock);
</script>
<!-- End Project Wonderful Ad Box Loader -->
